awk -f delay.awk sample.tr

awk -f pdr.awk sample.tr

xgraph -bg white -x time -y delay -lw 3 -t average_delay delay.tr

xgraph -bg white -x time -y pdr -lw 3 -t pdr pdr.tr

xgraph -bg white -x no. of nodes -y avg_energy -lw 3 -t average_energy energy.tr

xgraph -bg white -x no. of nodes -y throughput -lw 3 -t throughput throughput.tr


